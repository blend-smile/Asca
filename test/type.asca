type move;
type person;

func get_move(qword x:move) : ptr {
	if(x == 0) {
		return @"idle";
	} elif (x == 1) {
		return @"walk";
	} elif (x == 2) {
		return @"sit";
	} elif (x == 3) {
		return @"run";
	} else {
		return @"unknown";
	}	
}

func get_person(qword x:person) : ptr {
	if(x == 0) {
		return @"me";
	} elif (x == 1) {
		return @"you";
	} elif (x == 2) {
		return @"he";
	} else {
		return @"unknown";
	}

}
byte y:person = 1;
byte x:move = 2;

print(get_person(y));
print(get_move(x));

